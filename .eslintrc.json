{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "indent": ["error", 2],
    "semi": ["error", "never"],
    "quotes": ["error", "double"],
    "comma-dangle": ["error", "always-multiline"],
    "max-len": ["error", { "code":  140 }],
    "eol-last": ["error", "always"],
    "react-hooks/exhaustive-deps": ["off"]
  }
}
